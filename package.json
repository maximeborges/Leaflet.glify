    "glslify-loader": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.8.3",
    "point-in-polygon": "^1.0.1",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "raw-loader": "^0.5.1",
    "rbush": "^2.0.2",
    "sass-loader": "^6.0.7",
    "shader-loader": "git+https://git@github.com/jtwebb/shader-loader.git",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
