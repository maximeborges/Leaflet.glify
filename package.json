{
  "name": "leaflet.glify",
  "version": "1.0.0",
  "description": "web gl renderer plugin for leaflet",
  "main": "build.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npx webpack --mode=production --display-error-details --config config/webpack.config.prod.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --env.development --mode development --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtwebb/Leaflet.glify.git"
  },
  "authors": [
    "Robert Plummer <robertleeplummerjr@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jtwebb/Leaflet.glify/issues"
  },
  "keywords": [
    "leaflet",
    "gpu",
    "webgl",
    "web-gl",
    "web",
    "gl",
    "icon",
    "point",
    "fast"
  ],
  "homepage": "https://github.com/jtwebb/Leaflet.glify#readme",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.3.2",
    "css-loader": "^0.28.11",
